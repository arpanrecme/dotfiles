{
    "editor.suggestSelection": "first",
    "vsintellicode.modify.editor.suggestSelection": "automaticallyOverrodeDefaultValue",
    "workbench.editorAssociations": {
        "*.ipynb": "jupyter.notebook.ipynb"
    },
    "files.trimTrailingWhitespace": true,
    "java.project.importOnFirstTimeStartup": "automatic",
    "files.exclude": {
        "**/.classpath": true,
        "**/.project": true,
        "**/.settings": true,
        "**/.factorypath": true,
        "**/.git/objects/**": true,
        "**/.git/subtree-cache/**": true,
        "**/node_modules/*/**": true,
        "**/venv/**": true,
        "**/.venv/**": true,
        "**/env/**": true,
        "**/.env/**": true,
        "**/ENV/**": true,
        "**/env.bak/**": true,
        "**/venv.bak/**": true
    },
    "files.watcherExclude": {
        "**/.classpath": true,
        "**/.project": true,
        "**/.settings": true,
        "**/.factorypath": true,
        "**/.git/objects/**": true,
        "**/.git/subtree-cache/**": true,
        "**/node_modules/*/**": true,
        "**/venv/**": true,
        "**/.venv/**": true,
        "**/env/**": true,
        "**/.env/**": true,
        "**/ENV/**": true,
        "**/env.bak/**": true,
        "**/venv.bak/**": true
    },
    "redhat.telemetry.enabled": false,
    "explorer.confirmDelete": false,
    "editor.inlineSuggest.enabled": true,
    "git.enableSmartCommit": true,
    "security.workspace.trust.untrustedFiles": "open",
    "github.copilot.enable": {
        "*": false,
        "yaml": true,
        "plaintext": true,
        "markdown": true,
        "shellscript": true
    },
    "terminal.integrated.fontFamily": "'Hack Nerd Font'",
    "editor.mouseWheelZoom": true,
    "git.confirmSync": false,
    "git.autofetch": true,
    "explorer.confirmDragAndDrop": false,
    "java.autobuild.enabled": true,
    "java.configuration.updateBuildConfiguration": "automatic",
    "java.completion.enabled": true,
    "window.zoomLevel": 1,
    "diffEditor.ignoreTrimWhitespace": false,
    "remoteHub.logLevel": "debug",
    "editor.fontFamily": "'Cascadia Code', 'Hack Nerd Font', 'Droid Sans Mono', 'monospace', monospace, 'Droid Sans Fallback'",
    "git.ignoreLegacyWarning": true,
    "workbench.iconTheme": "material-icon-theme",
    "workbench.colorTheme": "Dracula",
    "[markdown]": {
        "editor.defaultFormatter": "DavidAnson.vscode-markdownlint",
        "files.trimTrailingWhitespace": false
    },
    "workbench.startupEditor": "none",
    "python.linting.enabled": true,
    "python.linting.pylintEnabled": true,
    "python.formatting.provider": "autopep8",
    "python.formatting.blackPath": "black",
    "python.formatting.autopep8Path": "autopep8",
    "python.formatting.autopep8Args": [
        "--indent-size=4",
        "--max-line-length=180"
    ],
    "python.formatting.blackArgs": [
        "--line-length",
        "180"
    ],
    "editor.tabSize": 4,
    "[xml]": {
        "editor.defaultFormatter": "redhat.vscode-xml"
    },
    "files.associations": {
        "**/tasks/**/*.yml": "ansible",
        "**/ansible/**/*.yml": "ansible",
        "site*.yml": "ansible",
        "**/*.tf": "terraform",
        "**/requirements.txt": "pip-requirements"
    },
    "ansible.validation": true,
    "ansible.ansibleLint.enabled": true,
    "ansible.ansible.path": "ansible",
    "html.format.wrapLineLength": 180,
    "terminal.integrated.persistentSessionScrollback": 1000,
    "yaml.format.printWidth": 180,
    "editor.wordWrap": "wordWrapColumn",
    "editor.wordWrapColumn": 180,
    "editor.minimap.enabled": false,
    "git.openDiffOnClick": true,
    "java.checkstyle.autocheck": true,
    "maven.dependency.enableConflictDiagnostics": true,
    "[java]": {
        "editor.suggest.snippetsPreventQuickSuggestions": false,
        "breadcrumbs.showArrays": true,
        "outline.showVariables": true,
        "editor.maxTokenizationLineLength": 20000,
        "editor.defaultFormatter": "redhat.java"
    },
    "java.format.settings.url": "https://raw.githubusercontent.com/google/styleguide/gh-pages/eclipse-java-google-style.xml",
    "java.format.settings.profile": "GoogleStyle",
    "[python]": {
        "editor.wordBasedSuggestions": false,
        "editor.defaultFormatter": "ms-python.python"
    },
    "telemetry.telemetryLevel": "off",
    "trunk.trunkGrayOutNonBlockingIssues": false,
    "[yaml]": {
        "editor.defaultFormatter": "redhat.vscode-yaml"
    },
    "java.jdt.ls.vmargs": "-XX:+UseParallelGC -XX:GCTimeRatio=4 -XX:AdaptiveSizePolicyWeight=90 -Dsun.zip.disableMemoryMapping=true -Xmx1G -Xms100m",
    "[shellscript]": {
        "editor.defaultFormatter": "foxundermoon.shell-format"
    },
    "[jsonc]": {
        "editor.defaultFormatter": "vscode.json-language-features"
    }
}